{
  "name": "styled-modules",
  "version": "0.1.2",
  "description": "CSS Modules support in Next.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/traveloka/styled-modules.git"
  },
  "keywords": [
    "css",
    "css-modules",
    "jsx",
    "next.js",
    "react",
    "soya",
    "soya-next"
  ],
  "files": [
    "lib",
    "babel.js",
    "loader.js",
    "server.js",
    "style.js"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "clean": "rimraf lib",
    "coverage": "jest --coverage",
    "coveralls": "yarn run coverage && cat ./coverage/lcov.info | coveralls",
    "prepublish": "yarn run clean && yarn run build",
    "test": "jest",
    "watch": "yarn build -- -w"
  },
  "dependencies": {
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-types": "^6.24.1",
    "loader-utils": "^1.1.0",
    "string-hash": "^1.1.3",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^2.13.1",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1"
  },
  "peerDependencies": {
    "css-loader": "^0.28.4",
    "react": "^15.5.4"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/test/"
    ]
  }
}
